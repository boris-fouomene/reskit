{
    "name": "@resk/core",
    "version": "1.0.3",
    "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
    "main": "build/index",
    "types": "build/index",
    "scripts": {
        "clear-npx-cache": "npx clear-npx-cache",
        "test": "npm run build && npx vitest",
        "test-node": "npm run build && node ./build/test/index.js",
        "build": "tsc && npx tsc-alias -p tsconfig.json",
        "start": "node build/test/index",
        "prepublish": "npm run build",
        "build-doc": "npx typedoc",
        "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
        "dev": "npx concurrently \"tsc --watch\" \"npx tsc-alias -p tsconfig.json\""
    },
    "devDependencies": {
        "@types/dotenv": "^6.1.1",
        "@types/lodash": "^4.17.10",
        "@types/node": "^22.7.5",
        "@types/qs": "^6.9.16",
        "copyfiles": "^2.4.1",
        "dotenv": "^16.4.5",
        "lerna": "^8.1.8",
        "openai": "^4.67.3",
        "reflect-metadata": "^0.2.2",
        "ts-alias": "^0.0.7",
        "ts-jest": "^29.2.5",
        "typedoc": "^0.26.9",
        "typescript": "^5.6.3",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.2"
    },
    "dependencies": {
        "crypto-es": "^2.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "qs": "^6.13.0",
        "stable-hash": "^0.0.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "TypeScript",
        "Decorators",
        "Modular",
        "Architecture",
        "Extensible",
        "Framework",
        "Custom",
        "Field",
        "Types",
        "Type",
        "Safety",
        "API",
        "Development",
        "Code",
        "Organization",
        "Application",
        "Scalability",
        "Dynamic",
        "Ecosystem",
        "Clean",
        "Code",
        "Object-Oriented",
        "Programming",
        "Developer-Friendly",
        "Open",
        "Source"
    ],
    "author": "Boris Fouomene",
    "npmClient": "npm"
}
