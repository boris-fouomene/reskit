        "Dynamic",
        "Ecosystem",
        "Clean",
        "Code",
        "Object-Oriented",
        "Programming",
        "Developer-Friendly",
        "Open",
        "Source"
    ],
    "author": "Boris Fouomene",
    "npmClient": "npm"
}