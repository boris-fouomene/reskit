{
  "name": "@resk/nativewind",
  "version": "1.0.0",
  "description": "Precompiled TypeScript UI framework built on NativeWind",
  "license": "ISC",
  "author": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./build/index.d.ts"
      ],
      "html": [
        "./build/html/index.d.ts"
      ]
    }
  },
  "files": [
    "build",
    "readme.md",
    "package.json",
    "html.js"
  ],
  "scripts": {
    "build": "tsc && npx tsc-alias -p tsconfig.json && npm run build:dts",
    "build:dts": "tsc -p tsconfig.dts.json --emitDeclarationOnly && npx tsc-alias -p tsconfig.dts.json",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "css:dev": "npx tailwindcss -i ./global-src.css -o ./global.css --watch",
    "css:build": "npx tailwindcss -i ./global-src.css -o ./global.css"
  },
  "keywords": [
    "react-native",
    "nativewind",
    "tailwindcss",
    "ui-framework"
  ],
  "peerDependencies": {
    "nativewind": "*",
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "nativewind": "^4.1.23",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.79.2"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.2.0"
  }
}