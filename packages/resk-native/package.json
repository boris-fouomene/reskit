{
  "name": "@resk/native",
  "main": "build/index",
  "types": "build/index",
  "version": "1.0.1",
  "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
  "scripts": {
    "build-core": "cd ../resk-core && npm run build",
    "build": "npm run build-core && cd ../resk-native && npm run build-me",
    "build-me": "tsc && tsc-alias -p tsconfig.json",
    "pretest": "npm run build-test",
    "build-test": "npm run build-core && cd ../resk-native && tsc --project ./tsconfig.test.json && npx tsc-alias -p tsconfig.test.json",
    "test": "npx jest"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@resk/core": "^1.5.0",
    "color": "^4.2.3",
    "tippy.js": "^6.3.7"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-is": ">=18",
    "@types/react-native-vector-icons": ">=6",
    "react": ">=18",
    "react-dom": ">=18",
    "react-is": ">=18",
    "react-native": ">=0.76",
    "react-native-web": ">=0.19"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/react": "~18.2.45",
    "tsc-alias": "^1.8.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "TypeScript",
    "Decorators",
    "Modular",
    "Architecture",
    "Extensible",
    "Framework",
    "Custom",
    "Field",
    "Types",
    "Type",
    "Safety",
    "API",
    "Development",
    "Code",
    "Organization",
    "Application",
    "Scalability",
    "Dynamic",
    "Ecosystem",
    "Clean",
    "Code",
    "Object-Oriented",
    "Programming",
    "Developer-Friendly",
    "Open",
    "Source",
    "react-native"
  ]
}
