{
  "extends": [
    "expo/tsconfig.base",
    "../../tsconfig.json"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./build",
    "paths": {
      "@resk/core": [
        "../resk-core"
      ],
      "@resk/core/*": [
        "../resk-core/*"
      ],
      "@/*": [
        "./*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@src/*": [
        "./src/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}