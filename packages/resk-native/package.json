{
  "name": "@resk/native",
  "main": "build/index",
  "types": "build/index",
  "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
  "scripts": {
    "build-core": "cd ../resk-core && npm run build",
    "build": "npm run build-core && cd ../resk-native && npm run build-me",
    "build-me": "tsc && tsc-alias -p tsconfig.json",
    "pretest": "npm run build-test",
    "build-test": "npm run build-core && cd ../resk-native && tsc --project ./tsconfig.test.json && npx tsc-alias -p tsconfig.test.json",
    "test": "npx jest"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@resk/core": "^1.4.2",
    "color": "^4.2.3",
    "react-fast-compare": "^3.2.2",
    "react-is": "^18.3.1",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-is": "^18.3.0",
    "@types/react-test-renderer": "^18.0.7",
    "concurrently": "^9.0.1",
    "jest": "^29.2.1",
    "react-test-renderer": "18.2.0",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "expo",
    "TypeScript",
    "Decorators",
    "Modular",
    "Architecture",
    "Extensible",
    "Framework",
    "Custom",
    "Field",
    "Types",
    "Type",
    "Safety",
    "API",
    "Development",
    "Code",
    "Organization",
    "Application",
    "Scalability",
    "Dynamic",
    "Ecosystem",
    "Clean",
    "Code",
    "Object-Oriented",
    "Programming",
    "Developer-Friendly",
    "Open",
    "Source",
    "react-native"
  ]
}
