{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./build",
    "composite": false,
    "incremental": false,
    "paths": {
      "@session": [
        "src/session"
      ],
      "@breakpoints/*": [
        "src/breakpoints/*"
      ],
      "@breakpoints": [
        "src/breakpoints"
      ],
      "@dimensions/*": [
        "src/dimensions/*"
      ],
      "@dimensions": [
        "src/dimensions"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@colors/*": [
        "src/theme/colors/*"
      ],
      "@colors": [
        "src/theme/colors"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@theme": [
        "src/theme"
      ],
      "@auth/*": [
        "src/auth/*"
      ],
      "@auth": [
        "src/auth"
      ],
      "@platform/*": [
        "src/platform/*"
      ],
      "@platform": [
        "src/platform"
      ],
      "@notify/*": [
        "src/notify/*"
      ],
      "@notify": [
        "src/notify"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@resk/core": [
        "../resk-core/build"
      ],
      "@shopify/flash-list": [
        "../../examples/resk-expo-docs/node_modules/@shopify/flash-list"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
}