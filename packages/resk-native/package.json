{
  "name": "@resk/native",
  "version": "1.13.2",
  "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
  "main": "./build/index",
  "module1": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "package.json",
    "readme.md"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc && npx tsc-alias -p tsconfig.json && npm run build-dts",
    "build-dts": "tsc -p tsconfig.dts.json --emitDeclarationOnly && npx tsc-alias -p tsconfig.dts.json",
    "test": "npx jest",
    "circular-deps": "madge --circular --extensions ts ./src/index.tsx"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.79.2",
    "react-native-vector-icons": "^10.2.0"
  },
  "dependencies": {
    "@resk/core": "^1.19.0",
    "@resk/native-icon-types": "^1.0.2",
    "color": "^5.0.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-is": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "ts-node": "^10.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "TypeScript",
    "Decorators",
    "Modular",
    "Architecture",
    "Extensible",
    "Framework",
    "Custom",
    "Field",
    "Types",
    "Type",
    "Safety",
    "API",
    "Development",
    "Code",
    "Organization",
    "Application",
    "Scalability",
    "Dynamic",
    "Ecosystem",
    "Clean",
    "Code",
    "Object-Oriented",
    "Programming",
    "Developer-Friendly",
    "Open",
    "Source",
    "react-native"
  ]
}
