{
  "name": "reskit-expo-stories",
  "version": "1.0.0",
  "private": true,
  "main": "index",
  "scripts": {
    "start": "npx expo start",
    "build-web": "cd ../resk-core && npm run build && cd ../resk-expo && npm run build && npx expo export -p web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "npx cross-env STORYBOOK_ENABLED='true' expo start -c",
    "storybook:ios": "npx cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "npx cross-env STORYBOOK_ENABLED='true' expo start --android"
  },
  "dependencies": {
    "@chromatic-com/storybook": "^3.1.0",
    "@expo/metro-runtime": "~4.0.0",
    "@expo/vector-icons": "^14.0.3",
    "@react-navigation/native": "^6.1.18",
    "@storybook/react": "^8.3.6",
    "@types/react": "~18.3.12",
    "expo": "^52.0.7",
    "expo-checkbox": "~4.0.0",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-router": "~4.0.6",
    "expo-sqlite": "~15.0.3",
    "expo-status-bar": "~2.0.0",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-screens": "~4.0.0",
    "react-native-web": "~0.19.10",
    "typescript": "~5.3.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@expo/metro-config": "^0.18.11",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-ondevice-actions": "^8.3.10",
    "@storybook/addon-ondevice-backgrounds": "^8.3.10",
    "@storybook/addon-ondevice-controls": "^8.3.10",
    "@storybook/addon-ondevice-knobs": "^7.6.20",
    "@storybook/addon-ondevice-notes": "^8.3.10",
    "@storybook/addon-react-native-web": "^0.0.26",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/react-native": "^8.3.10",
    "@types/react-native": "^0.72.8",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2"
  }
}
