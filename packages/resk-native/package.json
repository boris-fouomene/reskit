{
  "name": "@resk/native",
  "main": "build/index",
  "types": "../types-native/build/index.d.ts",
  "version": "1.4.5",
  "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
  "scripts": {
    "build-core": "cd ../resk-core && npm run build",
    "prepublish": "npm run build-me",
    "build": "npm run build-core && cd ../resk-native && npm run build-me",
    "build-me": "tsc && tsc-alias -p tsconfig.json",
    "pretest": "npm run build-test",
    "build-test": "npm run build-core && cd ../resk-native && tsc --project ./tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "test": "npx jest"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@resk/core": "^1.7.5",
    "@resk/types-native": "^1.0.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "color": "^4.2.3",
    "tippy.js": "^6.3.7"
  },
  "peerDependencies1": {
    "react-is": ">=18.0.0",
    "react-native": ">=0.76.0",
    "react-native-vector-icons": ">=6.0.0",
    "react-native-web": ">=0.19.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-is": "^18.0.0",
    "tsc-alias": "^1.8.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "TypeScript",
