{
  "extends": [
    "expo/tsconfig.base",
    "../../tsconfig.json"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./build",
    "paths": {
      "@resk/core": [
        "../resk-core/build/src"
      ],
      "@resk/core/*": [
        "../resk-core/build/src/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@theme": [
        "src/theme"
      ],
      "@platform/*": [
        "src/platform/*"
      ],
      "@platform": [
        "src/platform"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@src/*": [
        "./src/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}