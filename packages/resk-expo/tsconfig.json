{
  "extends": [
    "expo/tsconfig.base",
    "../../tsconfig.json"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./build",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@src/*": [
        "./src/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}