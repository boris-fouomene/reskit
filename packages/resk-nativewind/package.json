{
  "name": "@resk/nativewind",
  "version": "1.0.1-canary.18",
  "description": "Precompiled TypeScript UI framework built on NativeWind",
  "license": "ISC",
  "author": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "@resk/nativewind": "bin/index.js"
  },
  "files": [
    "build",
    "bin",
    "readme.md",
    "package.json",
    "html.js",
    "components",
    "theme-to-css-vars.js",
    "animations.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./*": "./build/*",
    "./components/*": "./components/*.js",
    "./components/activityIndicator": "./components/activityIndicator.js",
    "./components/alert": "./components/alert.js",
    "./components/avatar": "./components/avatar.js",
    "./components/backdrop": "./components/backdrop.js",
    "./components/backHandler": "./components/backHandler.js",
    "./components/badge": "./components/badge.js",
    "./components/bottomSheet": "./components/bottomSheet.js",
    "./components/button": "./components/button.js",
    "./components/checkbox": "./components/checkbox.js",
    "./components/countrySelector": "./components/countrySelector.js",
    "./components/dialog": "./components/dialog.js",
    "./components/divider": "./components/divider.js",
    "./components/drawer": "./components/drawer.js",
    "./components/expandable": "./components/expandable.js",
    "./components/helperText": "./components/helperText.js",
    "./components/icon": "./components/icon.js",
    "./components/keyboardAvoidingView": "./components/keyboardAvoidingView.js",
    "./components/keyboardEventHandler": "./components/keyboardEventHandler.js",
    "./components/menu": "./components/menu.js",
    "./components/modal": "./components/modal.js",
    "./components/nav": "./components/nav.js",
    "./components/portal": "./components/portal.js",
    "./components/progressBar": "./components/progressBar.js",
    "./components/slot": "./components/slot.js",
    "./components/stack": "./components/stack.js",
    "./components/surface": "./components/surface.js",
    "./components/switch": "./components/switch.js",
    "./components/textInput": "./components/textInput.js",
    "./components/tooltip": "./components/tooltip.js",
    "./html": "./build/html/index.js",
    "./variants": "./build/variants/index.js",
    "./utils": "./build/utils/index.js",
    "./i18n": "./build/i18n/index.js",
    "./platform": "./build/platform/index.js",
    "./theme-to-css-vars": "./theme-to-css-vars.js"
  },
  "typesVersions11": {
    "*": {
      ".": [
        "./build/index.d.ts"
      ],
      "html": [
        "./build/html/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npm run build-color-variants && npm run generate-variants && tsc && npx tsc-alias -p tsconfig.json && npm run build:dts && npm run generate-icon-types && npm run copy-css",
    "copy-css": "copyfiles -u 1 src/**/*.css build/",
    "build:dts": "tsc -p tsconfig.dts.json --emitDeclarationOnly && npx tsc-alias -p tsconfig.dts.json",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "css:dev": "npx tailwindcss -i ./global-src.css -o ./global.css --watch",
    "css:build": "npx tailwindcss -i ./global-src.css -o ./global.css",
    "generate-icon-types": "node generate-icon-types",
    "generate-variants": "node bin/generate-variants-dev",
    "build-color-variants": "tsc -p tsconfig.color-variants.json",
    "publish-preview": "npm publish --dry-run",
    "copy-build-to-expo-demo": "node bin/copy-build-to-expo-demo",
    "build:expo-demo": "npm run build && npm run copy-build-to-expo-demo",
    "publish:canary": "npm publish --tag canary"
  },
  "keywords": [
    "react-native",
    "nativewind",
    "tailwindcss",
    "ui-framework"
  ],
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "copyfiles": "^2.4.1",
    "nativewind": "^4.1.23",
    "react-native-vector-icons": "^10.2.0",
    "tailwind-variants": "^1.0.0"
  },
  "dependencies": {
    "@resk/core": "^1.21.4",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "tailwind-merge": "^3.2.0",
    "tippy.js": "^6.3.7"
  }
}
