{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./build",
    "paths": {
      "@breakpoints/*": [
        "src/breakpoints/*"
      ],
      "@breakpoints": [
        "src/breakpoints"
      ],
      "@dimensions/*": [
        "src/dimensions/*"
      ],
      "@dimensions": [
        "src/dimensions"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@colors/*": [
        "src/theme/colors/*"
      ],
      "@colors": [
        "src/theme/colors"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@theme": [
        "src/theme"
      ],
      "@auth/*": [
        "src/auth/*"
      ],
      "@auth": [
        "src/auth"
      ],
      "@platform/*": [
        "src/platform/*"
      ],
      "@platform": [
        "src/platform"
      ],
      "@notify/*": [
        "src/notify/*"
      ],
      "@notify": [
        "src/notify"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "react-native": [
        "../../examples/resk-expo-docs/node_modules/react-native",
      ],
      "react-native-svg": [
        "../../examples/resk-expo-docs/node_modules/react-native-svg",
      ],
      "expo-sqlite/*": [
        "../../examples/resk-expo-docs/node_modules/expo-sqlite/*",
      ],
      "@resk/core/*": [
        "../resk-core/build/*"
      ],
      "@resk/core": [
        "../resk-core/build/index"
      ],
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    //"src/components/Icon/Button.tsx",
    //"src/components/Datagrid/**/*",
    //"src/components/Filter/**/*",
    //"src/components/Form/**/*",
    //"src/components/Grid/**/*",
    //"src/components/Label/**/*",
  ],
  "exclude": [
    "../../resk-core/**/*",
    "**/resk-core/build/**/*",
    "react-native",
    "@resk/core",
    "./src/components/**/*",
    "../resk-core/**/*",
    "**/resk-core/**/*",
    "@resk/core/**/*",
    "react-native",
    "node_modules",
    "**/node_modules/*",
    "node_modules/**/*",
    "../../node_modules/**/*",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
  ]
}