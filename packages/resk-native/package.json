{
  "name": "@resk/native",
  "version": "1.11.5",
  "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
  "main": "./build/index",
  "module1": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "package.json",
    "readme.md"
  ],
  "scripts": {
    "build-core": "cd ../resk-core && npm run build",
    "prepublish": "npm run build-me",
    "build1": "tsup --config tsup.config.build.ts",
    "build": "tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json && npm run build-dts",
    "build-dts": "tsc -p tsconfig.dts.json && npx tsc-alias -p tsconfig.dts.json",
    "pretest": "npm run build-test",
    "build-test": "tsup --config tsup.config.test.ts",
    "test": "npx jest"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@resk/core": "^1.15.3",
    "color": "^4.2.3",
    "tippy.js": "^6.3.7"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-is": ">=18.0.0",
    "react-native": ">=0.76.0",
    "react-native-vector-icons": ">=10.0.0",
    "react-native-web": ">=0.19.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-is": "^18.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "tsc-alias": "^1.8.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "TypeScript",
    "Decorators",
    "Modular",
    "Architecture",
    "Extensible",
    "Framework",
    "Custom",
    "Field",
    "Types",
    "Type",
    "Safety",
    "API",
    "Development",
    "Code",
    "Organization",
    "Application",
    "Scalability",
    "Dynamic",
    "Ecosystem",
    "Clean",
    "Code",
    "Object-Oriented",
    "Programming",
    "Developer-Friendly",
    "Open",
    "Source",
    "react-native"
  ]
}
