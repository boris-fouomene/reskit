{
  "name": "@resk/nativewind",
  "version": "1.0.1-canary.0",
  "description": "Precompiled TypeScript UI framework built on NativeWind",
  "license": "ISC",
  "author": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "@resk/nativewind": "bin/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./build/index.d.ts"
      ],
      "html": [
        "./build/html/index.d.ts"
      ]
    }
  },
  "files": [
    "build",
    "bin",
    "readme.md",
    "package.json",
    "html.js",
    "theme-to-css-vars"
  ],
  "scripts": {
    "build": "npm run generate-color-variants && tsc && npx tsc-alias -p tsconfig.json && npm run build:dts && npm run generate-icon-types",
    "build:dts": "tsc -p tsconfig.dts.json --emitDeclarationOnly && npx tsc-alias -p tsconfig.dts.json",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "css:dev": "npx tailwindcss -i ./global-src.css -o ./global.css --watch",
    "css:build": "npx tailwindcss -i ./global-src.css -o ./global.css",
    "generate-icon-types": "node tests/cli.generate-icon-types",
    "generate-color-variants": "node bin/generate-color-variants-dev",
    "generate-color-variants:test": "node tests/cli.generate-color-variants",
    "publish-preview": "npm publish --dry-run",
    "copy-build-to-expo-demo": "node bin/copy-build-to-expo-demo",
    "build:expo-demo": "npm run build && npm run copy-build-to-expo-demo",
    "generate-padding-margin-variants": "node bin/generate-padding2marginVariants"
  },
  "keywords": [
    "react-native",
    "nativewind",
    "tailwindcss",
    "ui-framework"
  ],
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "nativewind": "^4.1.23",
    "react-native-vector-icons": "^10.2.0",
    "tailwind-variants": "^1.0.0"
  },
  "dependencies": {
    "@resk/core": "^1.20.0",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "execa": "^9.6.0",
    "tailwind-merge": "^3.2.0",
    "tippy.js": "^6.3.7"
  }
}
