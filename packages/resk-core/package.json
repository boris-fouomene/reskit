{
    "name": "@resk/core",
    "version": "1.12.2",
    "description": "An innovative TypeScript framework that empowers developers to build applications with a fully decorator-based architecture for efficient resource management. By combining the power of decorators with a resource-oriented design, DecorRes enhances code clarity, modularity, and maintainability.",
    "main": "./build/index.js",
    "module": "./build/index.mjs",
    "types": "./build/index.d.ts",
    "files": [
        "build"
    ],
    "typesVersions": {
        "*": {
            ".": [
                "./build/index.d.ts"
            ],
            "auth": [
                "./build/auth/index.d.ts"
            ],
            "countries": [
                "./build/countries/index.d.ts"
            ],
            "currency": [
                "./build/currency/index.d.ts"
            ],
            "decorators": [
                "./build/decorators/index.d.ts"
            ],
            "i18n": [
                "./build/i18n/index.d.ts"
            ],
            "inputFormatter": [
                "./build/inputFormatter/index.d.ts"
            ],
            "logger": [
                "./build/logger/index.d.ts"
            ],
            "observable": [
                "./build/observable/index.d.ts"
            ],
            "platform": [
                "./build/platform/index.d.ts"
            ],
            "resources": [
                "./build/resources/index.d.ts"
            ],
            "session": [
                "./build/session/index.d.ts"
            ],
            "utils": [
                "./build/utils/index.d.ts"
            ],
            "types": [
                "./build/types/index.d.ts"
            ],
            "validator": [
                "./build/validator/index.d.ts"
            ]
        }
    },
    "exports": {
        "./auth": {
            "import": "./build/auth/index.mjs",
            "require": "./build/auth/index.js"
        },
        "./countries": {
            "import": "./build/countries/index.mjs",
            "require": "./build/countries/index.js"
        },
        "./currency": {
            "import": "./build/currency/index.mjs",
            "require": "./build/currency/index.js"
        },
        "./decorators": {
            "import": "./build/decorators/index.mjs",
            "require": "./build/decorators/index.js"
        },
        "./fields": {
            "import": "./build/fields/index.mjs",
            "require": "./build/fields/index.js"
        },
        "./i18n": {
            "import": "./build/i18n/index.mjs",
            "require": "./build/i18n/index.js"
        },
        "./inputFormatter": {
            "import": "./build/inputFormatter/index.mjs",
            "require": "./build/inputFormatter/index.js"
        },
        "./logger": {
            "import": "./build/logger/index.mjs",
            "require": "./build/logger/index.js"
        },
        "./observable": {
            "import": "./build/observable/index.mjs",
            "require": "./build/observable/index.js"
        },
        "./platform": {
            "import": "./build/platform/index.mjs",
            "require": "./build/platform/index.js"
        },
        "./resources": {
            "import": "./build/resources/index.mjs",
            "require": "./build/resources/index.js"
        },
        "./session": {
            "import": "./build/session/index.mjs",
            "require": "./build/session/index.js"
        },
        "./utils": {
            "import": "./build/utils/index.mjs",
            "require": "./build/utils/index.js"
        },
        "./types": {
            "import": "./build/types/index.mjs",
            "require": "./build/types/index.js"
        },
        "./validator": {
            "import": "./build/validator/index.mjs",
            "require": "./build/validator/index.js"
        },
        "./*": {
            "import": "./build/*",
            "require": "./build/*"
        },
        ".": {
            "import": "./build/index.mjs",
            "require": "./build/index.js"
        }
    },
    "scripts": {
        "clear-npx-cache": "npx clear-npx-cache",
        "pretest": "npm run build-test",
        "build": "tsup --config tsup.config.build.ts",
        "build-dts": "tsc -p tsconfig.dts.json && npx tsc-alias -p tsconfig.dts.json",
        "build-test": "tsup --config tsup.config.test.ts",
        "dev": "tsup --config tsup.config.build.ts --watch",
        "test": "npx jest",
        "start": "node build/test/index",
        "prepublish": "npm run build",
        "build-doc": "npx typedoc",
        "check": "npm pack --dry-run",
        "types:publish": "npm publish ./types --access public"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/dotenv": "^6.1.1",
        "@types/google-libphonenumber": "^7.4.30",
        "@types/node": "^22.10.7",
        "@types/qs": "^6.9.18",
        "lerna": "^8.1.9",
        "typescript": "^5.8.2"
    },
    "dependencies": {
        "@types/color": "^4.2.0",
        "crypto-js": "^4.2.0",
        "google-libphonenumber": "^3.2.40",
        "i18n-js": "^4.5.1",
        "moment": "^2.30.1",
        "qs": "^6.14.0",
        "reflect-metadata": "^0.2.2",
        "stable-hash": "^0.0.5"
    },
    "peerDependenciesMeta": {},
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "TypeScript",
        "Decorators",
        "Modular",
        "Architecture",
        "Extensible",
