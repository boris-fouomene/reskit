{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./build",
    "paths": {
      "@session": [
        "src/session"
      ],
      "@dimensions/*": [
        "src/dimensions/*"
      ],
      "@dimensions": [
        "src/dimensions"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@colors/*": [
        "src/theme/colors/*"
      ],
      "@colors": [
        "src/theme/colors"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@theme": [
        "src/theme"
      ],
      "@platform/*": [
        "src/platform/*"
      ],
      "@platform": [
        "src/platform"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@src/*": [
        "src/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}