{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./build",
    "target": "ES2021",
    "module": "ESNext",
    "removeComments": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "resolveJsonModule": true,
    "incremental": true,
    "noLib": false,
    "sourceMap": false,
    "noEmitOnError": true,
    "noEmit": false,
    "typeRoots": [
      "./node_modules/@types",
    ],
    "types": [
      "jest",
      "node"
    ],
    "skipLibCheck": true,
    "composite": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": false,
    "declarationMap": false,
    "jsx": "react-jsx",
    "plugins": [
      {
        "transform": "typescript-transform-paths"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    "ts-paths-to-vite-alias.mjs",
    "packages/resk-tamagui/vite.config.mjs",
    "jest.config.js",
    "packages/resk-core/jest.config.js",
    "packages/resk-nest/src/locales/common.js"
  ],
  "exclude": [
    "./jest.config.js",
    "**/node_modules/*",
    "**/dist/*",
    "**/*.spec.ts",
    "**/build/**/*",
    "jest.config.*",
    "generate-typedoc",
    "**.test.ts",
    "**/*.test.tsx",
    "**/*.test.js",
    "**/*.test.jsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.spec.js",
    "**/*.spec.jsx"
  ],
  "extends": "expo/tsconfig.base"
}